+               if((priv->mod_clk[idx].reset_no == reg) && (priv->mod_clk[idx].reset_bit == bit) )
+               {
+                       dev_dbg(NULL, "deassert reset[%u] bit[%02u]\n", priv->mod_clk[idx].reset_no, priv->mod_clk[idx].reset_bit);
