+static const struct reset_control_ops cpg_mssr_reset_ops = {
+       .reset = cpg_mssr_reset,
+       .assert = cpg_mssr_assert,
+       .deassert = cpg_mssr_deassert,
+       .status = cpg_mssr_status,
+};
