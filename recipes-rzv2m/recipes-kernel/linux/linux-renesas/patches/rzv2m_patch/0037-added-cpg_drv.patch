+       CLK_TYPE_FF,            /* Fixed Factor Clock */
+       CLK_TYPE_FR,            /* Fixed Rate Clock */
+
+       /* Custom definitions start here */
+       CLK_TYP_STATIC,
+       CLK_TYPE_DIV,           /* Fixed Rate Clock */
+       CLK_TYPE_CUSTOM,
+};
+
+enum rst_types {
+       RST_NON,
+       RST_TYPEA,
+       RST_TYPEB,
