+	gpio_request(GPIO_FN_AVB_CRS, NULL);
+	gpio_request(GPIO_FN_AVB_GTXREFCLK, NULL);
+#endif
+   
+
+	val = readl(PUPR3);
+	val &= ~(PUPR3_ETH & ~PUPR3_ETH_MAGIC);
+	writel(val, PUPR3);
+
+#ifdef CONFIG_SH_MMCIF
+	/* IWG22M: eMMC: eMMC IOMUX customisation added*/
