+struct usbhs_private {
+	struct renesas_usbhs_platform_info info;
+	struct usb_phy *phy;
+	int pwen_gpio, id_gpio;
+    int pwr_gpio;
+};
+
+#define usbhs_get_priv(pdev) \
+	container_of(renesas_usbhs_get_info(pdev), struct usbhs_private, info)
+
+static int usbhs_power_ctrl(struct platform_device *pdev,
+				void __iomem *base, int enable)
+{
+	struct usbhs_private *priv = usbhs_get_priv(pdev);
+
+	if (!priv->phy)
+		return -ENODEV;
+
+	if (enable) {
+		int retval = usb_phy_init(priv->phy);
+
+		if (!retval)
+			retval = usb_phy_set_suspend(priv->phy, 0);
+		return retval;
+	}
+
+	usb_phy_set_suspend(priv->phy, 1);
+	usb_phy_shutdown(priv->phy);
+	return 0;
+}
+
+static int usbhs_hardware_init(struct platform_device *pdev)
+{
+	struct usbhs_private *priv = usbhs_get_priv(pdev);
+	struct usb_phy *phy;
+	int ret;
+	struct device_node *np, *np1;
+
+	np = of_find_node_by_path("/gpio@e6055000");
+	if (np) {
+		priv->id_gpio = of_get_gpio(np, 20);
+		of_node_put(np);
+	} else {
+		pr_warn("Error: Unable to get USB OTG ID input\n");
+		ret = -ENOTSUPP;
+		goto error2;
+	}
+
+	np1 = of_find_node_by_path("/gpio@e6055000");
+	if (np1) {
+		priv->pwen_gpio = of_get_gpio(np1, 18);
+		of_node_put(np1);
+	} else {
+		pr_warn("Error: Unable to get USB Power output pin\n");
+		ret = -ENOTSUPP;
+		goto error2;
+	}
+
+	/* Check USB OTG ID pin */
+	gpio_request_one(priv->id_gpio, GPIOF_IN, NULL);
+	gpio_request_one(priv->pwen_gpio, GPIOF_DIR_OUT, NULL); /* USB0_PWEN */
+	if (!gpio_get_value(priv->id_gpio)) {
+		gpio_set_value(priv->pwen_gpio, 1);
+		pr_warn("Error: USB0 cable selects host mode\n");
+		ret = -ENOTSUPP;
+		goto error;
+	}
+	else
+	{
+		gpio_set_value(priv->pwen_gpio, 0);
+	}
+
+	phy = usb_get_phy_dev(&pdev->dev, 0);
+	if (IS_ERR(phy)) {
+		ret = PTR_ERR(phy);
+		goto error;
+	}
+
+	priv->phy = phy;
+	return 0;
+ error:
+	gpio_free(priv->id_gpio);
+ error2:
+	return ret;
+}
+
+static int usbhs_hardware_exit(struct platform_device *pdev)
+{
+	struct usbhs_private *priv = usbhs_get_priv(pdev);
+
+	if (!priv->phy)
+		return 0;
+
+	usb_put_phy(priv->phy);
+	priv->phy = NULL;
+	gpio_free(priv->id_gpio);
+	return 0;
+}
