+#include "irqs.h"
+#include "r8a7742.h"
+#include "rcar-gen2.h"
+
+static const struct resource pfc_resources[] __initconst = {
+	DEFINE_RES_MEM(0xe6060000, 0x250),
+};
+
+#define r8a7742_register_pfc()						\
+	platform_device_register_simple("pfc-r8a7742", -1, pfc_resources, \
+					ARRAY_SIZE(pfc_resources))
+
+#define R8A7742_GPIO(idx)						\
+static const struct resource r8a7742_gpio##idx##_resources[] __initconst = { \
+	DEFINE_RES_MEM(0xe6050000 + 0x1000 * (idx), 0x50),		\
+	DEFINE_RES_IRQ(gic_spi(4 + (idx))),				\
+};									\
+									\
+static const struct gpio_rcar_config					\
+r8a7742_gpio##idx##_platform_data __initconst = {			\
+	.gpio_base	= 32 * (idx),					\
+	.irq_base	= 0,						\
+	.number_of_pins	= 32,						\
+	.pctl_name	= "pfc-r8a7742",				\
+	.has_both_edge_trigger = 1,					\
+};									\
+
+R8A7742_GPIO(0);
+R8A7742_GPIO(1);
+R8A7742_GPIO(2);
+R8A7742_GPIO(3);
+R8A7742_GPIO(4);
+R8A7742_GPIO(5);
+
+#define r8a7742_register_gpio(idx)					\
+	platform_device_register_resndata(&platform_bus, "gpio_rcar", idx, \
+		r8a7742_gpio##idx##_resources,				\
+		ARRAY_SIZE(r8a7742_gpio##idx##_resources),		\
+		&r8a7742_gpio##idx##_platform_data,			\
+		sizeof(r8a7742_gpio##idx##_platform_data))
+
+static struct resource i2c_resources[] __initdata = {
+	/* I2C0 */
+	DEFINE_RES_MEM(0xE6508000, 0x40),
+	DEFINE_RES_IRQ(gic_spi(287)),
+	/* I2C1 */
+	DEFINE_RES_MEM(0xE6518000, 0x40),
+	DEFINE_RES_IRQ(gic_spi(288)),
+	/* I2C2 */
+	DEFINE_RES_MEM(0xE6530000, 0x40),
+	DEFINE_RES_IRQ(gic_spi(286)),
+	/* I2C3 */
+	DEFINE_RES_MEM(0xE6540000, 0x40),
+	DEFINE_RES_IRQ(gic_spi(290)),
+
+};
+
+#define r8a7742_register_i2c(idx)		\
+	platform_device_register_simple(	\
+		"i2c-rcar_gen2", idx,		\
+		i2c_resources + (2 * idx), 2);	\
+
+void __init r8a7742_pinmux_init(void)
+{
+	r8a7742_register_pfc();
+	r8a7742_register_gpio(0);
+	r8a7742_register_gpio(1);
+	r8a7742_register_gpio(2);
+	r8a7742_register_gpio(3);
+	r8a7742_register_gpio(4);
+	r8a7742_register_gpio(5);
+}
+
+#define __R8A7742_SCIF(scif_type, _scscr, index, baseaddr, irq)		\
+static struct plat_sci_port scif##index##_platform_data = {		\
+	.type		= scif_type,					\
+	.flags		= UPF_BOOT_AUTOCONF | UPF_IOREMAP,		\
+	.scscr		= _scscr,					\
+};									\
+									\
+static struct resource scif##index##_resources[] = {			\
+	DEFINE_RES_MEM(baseaddr, 0x100),				\
+	DEFINE_RES_IRQ(irq),						\
+}
+
+#define R8A7742_SCIF(index, baseaddr, irq)				\
+	__R8A7742_SCIF(PORT_SCIF, SCSCR_RE | SCSCR_TE,			\
+		       index, baseaddr, irq)
+
+#define R8A7742_SCIFA(index, baseaddr, irq)				\
+	__R8A7742_SCIF(PORT_SCIFA, SCSCR_RE | SCSCR_TE | SCSCR_CKE0,	\
+		       index, baseaddr, irq)
+
+#define R8A7742_SCIFB(index, baseaddr, irq)				\
+	__R8A7742_SCIF(PORT_SCIFB, SCSCR_RE | SCSCR_TE,			\
+		       index, baseaddr, irq)
+
+#define R8A7742_HSCIF(index, baseaddr, irq)				\
+	__R8A7742_SCIF(PORT_HSCIF, SCSCR_RE | SCSCR_TE,			\
+		       index, baseaddr, irq)
+
+R8A7742_SCIFA(0, 0xe6c40000, gic_spi(144)); /* SCIFA0 */
+R8A7742_SCIFA(1, 0xe6c50000, gic_spi(145)); /* SCIFA1 */
+R8A7742_SCIFB(2, 0xe6c20000, gic_spi(148)); /* SCIFB0 */
+R8A7742_SCIFB(3, 0xe6c30000, gic_spi(149)); /* SCIFB1 */
+R8A7742_SCIFB(4, 0xe6ce0000, gic_spi(150)); /* SCIFB2 */
+R8A7742_SCIFA(5, 0xe6c60000, gic_spi(151)); /* SCIFA2 */
+R8A7742_SCIF(6,  0xe6e60000, gic_spi(152)); /* SCIF0 */
+R8A7742_SCIF(7,  0xe6e68000, gic_spi(153)); /* SCIF1 */
+R8A7742_HSCIF(8, 0xe62c0000, gic_spi(154)); /* HSCIF0 */
+R8A7742_HSCIF(9, 0xe62c8000, gic_spi(155)); /* HSCIF1 */
+
+#define r8a7742_register_scif(index)					       \
+	platform_device_register_resndata(&platform_bus, "sh-sci", index,      \
+					  scif##index##_resources,	       \
+					  ARRAY_SIZE(scif##index##_resources), \
+					  &scif##index##_platform_data,	       \
+					  sizeof(scif##index##_platform_data))
+
+static const struct renesas_irqc_config irqc0_data __initconst = {
+	.irq_base = irq_pin(0), /* IRQ0 -> IRQ3 */
+};
+
+static const struct resource irqc0_resources[] __initconst = {
+	DEFINE_RES_MEM(0xe61c0000, 0x200), /* IRQC Event Detector Block_0 */
+	DEFINE_RES_IRQ(gic_spi(0)), /* IRQ0 */
+	DEFINE_RES_IRQ(gic_spi(1)), /* IRQ1 */
+	DEFINE_RES_IRQ(gic_spi(2)), /* IRQ2 */
+	DEFINE_RES_IRQ(gic_spi(3)), /* IRQ3 */
+};
+
+#define r8a7742_register_irqc(idx)					\
+	platform_device_register_resndata(&platform_bus, "renesas_irqc", \
+					  idx, irqc##idx##_resources,	\
+					  ARRAY_SIZE(irqc##idx##_resources), \
+					  &irqc##idx##_data,		\
+					  sizeof(struct renesas_irqc_config))
+
+static const struct resource thermal_resources[] __initconst = {
+	DEFINE_RES_MEM(0xe61f0000, 0x14),
+	DEFINE_RES_MEM(0xe61f0100, 0x38),
+	DEFINE_RES_IRQ(gic_spi(69)),
+};
+
+#define r8a7742_register_thermal()					\
+	platform_device_register_simple("rcar_thermal", -1,		\
+					thermal_resources,		\
+					ARRAY_SIZE(thermal_resources))
+
+static const struct sh_timer_config cmt00_platform_data __initconst = {
+	.name = "CMT00",
+	.timer_bit = 0,
+	.clockevent_rating = 80,
+};
+
+static const struct resource cmt00_resources[] __initconst = {
+	DEFINE_RES_MEM(0xffca0510, 0x0c),
+	DEFINE_RES_MEM(0xffca0500, 0x04),
+	DEFINE_RES_IRQ(gic_spi(142)), /* CMT0_0 */
+};
+
+#define r8a7742_register_cmt(idx)					\
+	platform_device_register_resndata(&platform_bus, "sh_cmt",	\
+					  idx, cmt##idx##_resources,	\
+					  ARRAY_SIZE(cmt##idx##_resources), \
+					  &cmt##idx##_platform_data,	\
+					  sizeof(struct sh_timer_config))
+
+/* MSIOF */
+#define MSIOF_COMMON				\
+	.rx_fifo_override	= 256,			\
+	.num_chipselect		= 1
+
+static const struct sh_msiof_spi_info sh_msiof_info[] __initconst = {
+	{
+		MSIOF_COMMON,
+	},
+	{
+		MSIOF_COMMON,
+	},
+	{
+		MSIOF_COMMON,
+	},
+	{
+		MSIOF_COMMON,
+	},
+};
+
+static const struct resource sh_msiof0_resources[] __initconst = {
+	DEFINE_RES_MEM(0xe6e20000, 0x0064),
+	DEFINE_RES_IRQ(gic_spi(156)),
+};
+
+static const struct resource sh_msiof1_resources[] __initconst = {
+	DEFINE_RES_MEM(0xe6e10000, 0x0064),
+	DEFINE_RES_IRQ(gic_spi(157)),
+};
+
+static const struct resource sh_msiof2_resources[] __initconst = {
+	DEFINE_RES_MEM(0xe6e00000, 0x0064),
+	DEFINE_RES_IRQ(gic_spi(158)),
+};
+
+static const struct resource sh_msiof3_resources[] __initconst = {
+	DEFINE_RES_MEM(0xe6c90000, 0x0064),
+	DEFINE_RES_IRQ(gic_spi(159)),
+};
+
+#define r8a7742_register_msiof(idx)					\
+	platform_device_register_resndata(&platform_bus, "spi_sh_msiof", \
+				  (idx+1), sh_msiof##idx##_resources,	\
+				  ARRAY_SIZE(sh_msiof##idx##_resources), \
+				  &sh_msiof_info[idx],		\
+				  sizeof(struct sh_msiof_spi_info))
+
+/* POWERVR */
+static const struct resource powervr_resources[] __initconst = {
+	DEFINE_RES_MEM(0xfd000000, 0x10000),
+	DEFINE_RES_IRQ(gic_spi(119)),
+};
+
+#define __r8a7742_register_pvrsrvkm()					\
+	platform_device_register_simple("pvrsrvkm", -1,			\
+					powervr_resources,		\
+					ARRAY_SIZE(powervr_resources))
+
+#define CPG_BASE	0xe6150000
+#define CPG_LEN		0x1000
+#define RGXCR		0x0B4
+
+void __init r8a7742_register_pvrsrvkm(void)
+{
+	void __iomem *cpg_base;
+	unsigned int val;
+
+	cpg_base = ioremap(CPG_BASE, CPG_LEN);
+	val = ioread32(cpg_base + RGXCR);
+	iowrite32(val | (1 << 16), cpg_base + RGXCR);
+	iounmap(cpg_base);
+	__r8a7742_register_pvrsrvkm();
+}
+
+/* SSP */
+void __init r8a7742_register_ssp(void)
+{
+	struct platform_device_info info = {
+		.name = "ssp_dev",
+		.id = -1,
+	};
+	platform_device_register_full(&info);
+}
+
+void __init r8a7742_add_dt_devices(void)
+{
+	r8a7742_pm_init();
+	r8a7742_init_pm_domains();
+	r8a7742_register_cmt(00);
+	r8a7742_register_pvrsrvkm();
+	r8a7742_register_ssp();
+}
+
+void __init r8a7742_add_standard_devices(void)
+{
+	r8a7742_register_scif(0);
+	r8a7742_register_scif(1);
+	r8a7742_register_scif(2);
+	r8a7742_register_scif(3);
+	r8a7742_register_scif(4);
+	r8a7742_register_scif(5);
+	r8a7742_register_scif(6);
+	r8a7742_register_scif(7);
+	r8a7742_register_scif(8);
+	r8a7742_register_scif(9);
+	r8a7742_add_dt_devices();
+	r8a7742_register_irqc(0);
+	r8a7742_register_thermal();
+	r8a7742_register_i2c(0);
+	r8a7742_register_i2c(1);
+	r8a7742_register_i2c(2);
+	r8a7742_register_i2c(3);
+	r8a7742_register_msiof(0);
+	r8a7742_register_msiof(1);
+	r8a7742_register_msiof(2);
+	r8a7742_register_msiof(3);
+}
+
+#ifdef CONFIG_USE_OF
+
+static const char * const r8a7742_boards_compat_dt[] __initconst = {
+	"renesas,r8a7742",
+	NULL,
+};
+
+DT_MACHINE_START(R8A7742_DT, "Generic R8A7742 (Flattened Device Tree)")
+	.smp		= smp_ops(r8a7742_smp_ops),
+	.init_early	= shmobile_init_delay,
+	.init_time	= rcar_gen2_timer_init,
+	.init_late	= shmobile_init_late,
+	.reserve	= rcar_gen2_reserve,
+	.dt_compat	= r8a7742_boards_compat_dt,
+MACHINE_END
+#endif /* CONFIG_USE_OF */
