+
+/ {
+	model = "iW-RainboW-G21M-Q7 Board based on Renesas RZ/G1H";
+	compatible = "iw,iwg21m", "renesas,r8a7742";
+
+	aliases {
+		serial0 = &scif0;
+		serial1 = &scif1;
+		serial2 = &scifa2;
+		serial3 = &scifb1;
+		serial4 = &scifb2;
+		serial5 = &hscif0;
+		sata0	= &sata0;
+		sata1	= &sata1;
+		xhci	= &xhci;
+		pciec	= &pciec;
+		pcie_clk = &pcie_bus_clk;
+	};
+
+	memory@40000000 {
+		device_type = "memory";
+		reg = <0 0x40000000 0 0x40000000>;
+	};
+
+	memory@200000000 {
+		device_type = "memory";
+		reg = <2 0x00000000 0 0x40000000>;
+	};
+
+	lbsc {
+		#address-cells = <1>;
+		#size-cells = <1>;
+	};
+
+	reg_1p0v: 1p0v {
+		compatible = "regulator-fixed";
+		regulator-name = "1P0V";
+		regulator-min-microvolt = <1000000>;
+		regulator-max-microvolt = <1000000>;
+		regulator-boot-on;
+		regulator-always-on;
+	};
+
+	reg_2p5v: fixedregulator@0 {
+		compatible = "regulator-fixed";
+		regulator-name = "fixed-2.5V";
+		regulator-min-microvolt = <2500000>;
+		regulator-max-microvolt = <2500000>;
+		regulator-boot-on;
+		regulator-always-on;
+	};
+
+	reg_3p3v: fixedregulator@1 {
+		compatible = "regulator-fixed";
+		regulator-name = "fixed-3.3V";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+		regulator-boot-on;
+		regulator-always-on;
+	};
+
+	vcc_sdhi2: regulator@1 {
+		compatible = "regulator-fixed";
+		regulator-name = "SDHI2 Vcc";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+		gpios = <&gpio1 27 GPIO_ACTIVE_LOW>;
+		enable-active-high;
+	};
+
+	vccq_sdhi2: regulator@2 {
+		compatible = "regulator-gpio";
+		regulator-name = "SDHI2 VccQ";
+		regulator-min-microvolt = <1800000>;
+		regulator-max-microvolt = <3300000>;
+		gpios = <&gpio1 8 GPIO_ACTIVE_HIGH>;
+		gpios-states = <1>;
+		states = <3300000 1
+			  1800000 0>;
+	};
+
+	sound {
+		compatible = "simple-audio-card";
+
+		simple-audio-card,format = "i2s";
+
+		simple-audio-card,routing =
+			"MIC_IN", "Mic Jack",
+			"Mic Jack", "Mic Bias",
+			"Headphone Jack", "HP_OUT";
+
+		sndcpu: simple-audio-card,cpu {
+			sound-dai = <&rcar_sound>;
+		};
+
+		sndcodec: simple-audio-card,codec {
+			sound-dai = <&codec>;
+			bitclock-master;
+			frame-master;
+			system-clock-frequency = <26000000>;
+		};
+	};
+
+	buzzer {
+		compatible = "pwm-backlight";
+		pwms = <&tpu 0 5000000 GPIO_ACTIVE_HIGH>;
+		brightness-levels = <0 4 8 16 32 64 128 255>;
+		default-brightness-level = <7>;
+		pinctrl-0 = <&buzzer_pins>;
+		pinctrl-names = "default";
+	};
+
+	fan {
+		compatible = "pwm-backlight";
+		pwms = <&tpu 1 5000000 GPIO_ACTIVE_HIGH>;
+		brightness-levels = <0 4 8 16 32 64 128 255>;
+		default-brightness-level = <7>;
+		pinctrl-0 = <&fan_pins>;
+		pinctrl-names = "default";
+	};
+
+	backlight {
+		compatible = "pwm-backlight";
+		pwms = <&tpu 2 5000000 GPIO_ACTIVE_HIGH>;
+		brightness-levels = <0 4 8 16 32 64 128 255>;
+		default-brightness-level = <7>;
+		pinctrl-0 = <&backlight_pins>;
+		pinctrl-names = "default";
+	};
+
+        lvds0:lvds_qseven@0 {
+                compatible = "iwave,lvds-qseven";
+                backlgt-gpios = <&gpio3 11 GPIO_ACTIVE_HIGH>;
+                poweron-gpios = <&gpio5 28 GPIO_ACTIVE_HIGH>;
+                status = "okay";
+        };
+
+        lvds1:lvds_qseven@1 {
+                compatible = "iwave,lvds-qseven";
+                backlgt-gpios = <&gpio2 17 GPIO_ACTIVE_HIGH>;
+                poweron-gpios = <&gpio4 2 GPIO_ACTIVE_HIGH>;
+                status = "disabled";
+        };
+
+        iwg21m_q7_common {
+		compatible = "iwave,g21m-q7-com";
+		som-rev = <0>;
+		vin0-status = <1>;
+		vin1-status = <1>;
+		vin2-status = <1>;
+		vin0-ov5640 = <1>;
+		vin1-ov5640 = <1>;
+		vin2-ov5640 = <1>;
+		vin3-ov5640 = <1>;
+		cam0-pwr = <&gpio1 13 GPIO_ACTIVE_HIGH>;
+		cam2-pwr = <&gpio2 27 GPIO_ACTIVE_HIGH>;
+		status = "okay";
+	};
+};
+
+&extal_clk {
+	clock-frequency = <20000000>;
