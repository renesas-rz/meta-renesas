+	};
+};
+
+&msiof0 {
+        pinctrl-0 = <&msiof0_pins>;
+        pinctrl-names = "default";
+
+        status = "disabled";
+
+        flash1: flash@1 {
+                #address-cells = <1>;
+                #size-cells = <1>;
+                compatible = "sst,sst25vf016b";
+                reg = <1>;
+                spi-max-frequency = <30000000>;
+                spi-tx-bus-width = <1>;
+                spi-rx-bus-width = <1>;
+                m25p,fast-read;
+                spi-cpol;
+                spi-cpha;
+        };
+
+};
