From 25b97d80ef52d2f6a58d13e8d64e1574c482113b Mon Sep 17 00:00:00 2001
From: Duy Nguyen <duynt26@fsoft.com.vn>
Date: Tue, 8 Oct 2019 09:00:01 +0700
Subject: [PATCH 219/559] ARM: DTS: iwg23s: add Ethernet AVB and PHY device

Signed-off-by: Quoc Ngo <quoc.ngo.wz@rvc.renesas.com>
Signed-off-by: vietn <vietn@fsoft.com.vn>
Signed-off-by: ChinhPC <chinhpc@fsoft.com.vn>
Signed-off-by: Duy Nguyen <duynt26@fsoft.com.vn>
---
 arch/arm/boot/dts/r8a77470-iwg23s-sbc.dts | 9 +++++++--
 1 file changed, 7 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/r8a77470-iwg23s-sbc.dts b/arch/arm/boot/dts/r8a77470-iwg23s-sbc.dts
index fdec72e..d6753c9 100644
--- a/arch/arm/boot/dts/r8a77470-iwg23s-sbc.dts
+++ b/arch/arm/boot/dts/r8a77470-iwg23s-sbc.dts
@@ -129,8 +129,13 @@
 
 &pfc {
 	avb_pins: avb {
-		groups = "avb_mdio", "avb_gmii_tx_rx";
-		function = "avb";
+		renesas,groups = "avb_mdio", "avb_gmii_tx_rx", "avb_phy_int";
+		renesas,function = "avb";
+	};
+
+	phy0_pins: phy0 {
+		renesas,groups = "intc_irq8";
+		renesas,function = "intc";
 	};
 
 	i2c3_pins: i2c3 {
-- 
