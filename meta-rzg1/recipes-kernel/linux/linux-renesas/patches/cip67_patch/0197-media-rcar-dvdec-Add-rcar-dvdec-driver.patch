+		state->base + DVDEC_HAFCCR3_REG);
+	iowrite16(rcar_dvdec_reg_color[dvdec_fmt].VCDWCR1,
+		state->base + DVDEC_VCDWCR1_REG);
+	iowrite16(rcar_dvdec_reg_color[dvdec_fmt].ACCCR1,
