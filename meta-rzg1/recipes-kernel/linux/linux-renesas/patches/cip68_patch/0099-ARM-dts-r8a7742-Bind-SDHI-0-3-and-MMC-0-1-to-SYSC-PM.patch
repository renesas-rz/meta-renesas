From 1006a97a890a13aabdddd3f33d65f3af42ccebc2 Mon Sep 17 00:00:00 2001
From: Loc Vu <loc.vu.zn@renesas.com>
Date: Mon, 12 Oct 2020 18:08:57 +0700
Subject: [PATCH 099/433] ARM: dts: r8a7742: Bind SDHI[0-3] and MMC[0-1] to
 SYSC PM domains

Signed-off-by: Loc Vu <loc.vu.zn@renesas.com>
---
 arch/arm/boot/dts/r8a7742.dtsi | 12 ++++++------
 1 file changed, 6 insertions(+), 6 deletions(-)

diff --git a/arch/arm/boot/dts/r8a7742.dtsi b/arch/arm/boot/dts/r8a7742.dtsi
index 6125ec2..ea77351 100644
--- a/arch/arm/boot/dts/r8a7742.dtsi
+++ b/arch/arm/boot/dts/r8a7742.dtsi
@@ -1811,7 +1811,7 @@
 			       <&dmac1 0xcd>, <&dmac1 0xce>;
 			dma-names = "tx", "rx", "tx", "rx";
 			max-frequency = <195000000>;
-			power-domains = <&cpg_clocks>;
+			power-domains = <&sysc R8A7742_PD_ALWAYS_ON>;
 			status = "disabled";
 		};
 
@@ -1825,7 +1825,7 @@
 			       <&dmac1 0xc9>, <&dmac1 0xca>;
 			dma-names = "tx", "rx", "tx", "rx";
 			max-frequency = <195000000>;
-			power-domains = <&cpg_clocks>;
+			power-domains = <&sysc R8A7742_PD_ALWAYS_ON>;
 			status = "disabled";
 		};
 
@@ -1839,7 +1839,7 @@
 			       <&dmac1 0xc1>, <&dmac1 0xc2>;
 			dma-names = "tx", "rx", "tx", "rx";
 			max-frequency = <97500000>;
-			power-domains = <&cpg_clocks>;
+			power-domains = <&sysc R8A7742_PD_ALWAYS_ON>;
 			renesas,mmc-scc-tapnum = <8>;
 			sd-uhs-sdr50;
 			status = "disabled";
@@ -1855,7 +1855,7 @@
 			       <&dmac1 0xd3>, <&dmac1 0xd4>;
 			dma-names = "tx", "rx", "tx", "rx";
 			max-frequency = <97500000>;
-			power-domains = <&cpg_clocks>;
+			power-domains = <&sysc R8A7742_PD_ALWAYS_ON>;
 			status = "disabled";
 		};
 
@@ -1868,7 +1868,7 @@
 			dmas = <&dmac0 0xd1>, <&dmac0 0xd2>,
 			       <&dmac1 0xd1>, <&dmac1 0xd2>;
 			dma-names = "tx", "rx", "tx", "rx";
-			power-domains = <&cpg_clocks>;
+			power-domains = <&sysc R8A7742_PD_ALWAYS_ON>;
 			reg-io-width = <4>;
 			status = "disabled";
 			max-frequency = <97500000>;
@@ -1883,7 +1883,7 @@
 			dmas = <&dmac0 0xe1>, <&dmac0 0xe2>,
 			       <&dmac1 0xe1>, <&dmac1 0xe2>;
 			dma-names = "tx", "rx", "tx", "rx";
-			power-domains = <&cpg_clocks>;
+			power-domains = <&sysc R8A7742_PD_ALWAYS_ON>;
 			reg-io-width = <4>;
 			status = "disabled";
 			max-frequency = <97500000>;
